{
	"name": "bk-publisher",
	"version": "1.0.0",
	"description": "Publish your notes directly to WordPress as posts or pages.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"wordpress",
		"publish",
		"markdown"
	],
	"author": "Bill A. King",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^18.0.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
